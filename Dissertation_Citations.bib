	keywords = {Global Warming, Uncertainty, Animal Husbandry, Animals, Biomass, Carbon, Carbon Sequestration, Conservation of Natural Resources, Forestry, Forests, Human Activities, Internationality, Plants, Trees, Tropical Climate},
