	pages = {114},
	file = {Voigt et al. - 2021 - Cleaner burning aviation fuels can reduce contrail.pdf:C\:\\Users\\mike_\\Zotero\\storage\\5Q8L7H2E\\Voigt et al. - 2021 - Cleaner burning aviation fuels can reduce contrail.pdf:application/pdf},
