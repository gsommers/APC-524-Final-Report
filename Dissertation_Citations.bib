	journal = {ACS Sustainable Chemistry \& Engineering},
	author = {Shi, Rui and Ukaew, Suchada and Archer, David W. and Lee, Joon Hee and Pearlson, Matthew N. and Lewis, Kristin C. and Shonnard, David R.},
